                            </div>
                              <%
      String DB_URL = "jdbc:mysql://localhost:3306/gtpoint";
      String DB_USER = "root";
      String DB_PASSWORD = "onlyroot";
      Connection conn;
      Statement stmt;
      ResultSet rs;
      String query = "select * from provisionpt where userId=123";
      
      try {
         Class.forName("com.mysql.jdbc.Driver");
         conn = DriverManager.getConnection(DB_URL, DB_USER, DB_PASSWORD);
         stmt = conn.createStatement();
         rs = stmt.executeQuery(query);
      %>
         <div class="bootstrap-data-table-panel">
                                <div class="table-responsive">
                                    <table id="row-select" class="display table table-borderd table-hover">
                                        <thead>
         									<tr>
         										<th>날짜</th>
         										<th>진행 테스트</th>
         										<th>포인트</th>
  									       	</tr>
										</thead>
 										<tbody>
         								<% 
         									while(rs.next()){%>
         								<tr>
         									<td><%=rs.getInt("provisionDate")%></td>
         									<td><%=rs.getString("aboutTest")%></td>
         									<td><%=rs.getInt("provisionPt")%></td>
         								</tr>
         								<%
         								}
         								%>
         								</tbody>
										<tfoot>
											<tr>
												<th>날짜</th>
         										<th>진행 테스트</th>
         										<th>포인트</th>
											</tr>
										</tfoot>
									</table>
         	<%
         	rs.close();
         	stmt.close();
         	conn.close();
      } catch (Exception e) {
    	  e.printStackTrace();
      }
 %>
